// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model test {
  id   Int     @id @default(autoincrement())
  name String?
}

model events {
  id    Int    @id @default(autoincrement())
  title String
  body  String
}

// SELECT id, contactfname, contactlname,
// addr1, addr2, addr3, addr4, tel, email,
// website, mapref FROM hbtest.breeder where id = 3;
model breeder {
  id          Int    @id @default(autoincrement())
  breedername String
  addr1       String
  tel         String
  email       String
  website     String
  mapref      String
}

// SELECT hprse_id, name,  birthyear FROM storehorse limit 6
model storehorse {
  horse_id  Int    @id @default(autoincrement())
  name      String
  birthyear Int
}
